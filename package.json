{
  "name": "folding-sample",
  "displayName": "Folding Sample",
  "description": "A sample VS Code extension to demonstrate custom text folding",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:plaintext",
    "onCommand:extension.startFolding"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.foldSelection",
        "title": "Get Selection Range"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "eslint": "^8.43.0",
    "glob": "^8.1.0",
    "mocha": "^10.0.0",
    "typescript": "^5.1.6",
    "vscode-test": "^1.8.0"
  },
  "dependencies": {}
}